{
    "name": "tayeb-ali/larazoul",
    "description": "LaraZoul new Laravel cms and Crud package",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Elteyab Hassan",
            "email": "me@elteyab.sd"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravelcollective/html": "^6.3",
        "mcamara/laravel-localization": "^1.7",
        "laravel/helpers": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "larazoul\\Larazoul\\": "src/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "orchestra/testbench-browser-kit": "~3.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Larazoul\\Larazoul\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larazoul\\Larazoul\\Providers\\LarazoulServiceProvider"
            ]
        }
    }
}
